{
  "name": "new-folder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "architecture:baseline": "node tools/checks/architecture-boundaries.mjs --write-baseline",
    "architecture:check": "node tools/checks/architecture-boundaries.mjs --check-baseline",
    "filesize:baseline": "node tools/checks/file-size-budget.mjs --write-baseline",
    "filesize:check": "node tools/checks/file-size-budget.mjs --check-baseline",
    "content:build:resources": "node tools/content-build/build-resources-registry.mjs --write",
    "content:build:processes": "node tools/content-build/build-processes-registry.mjs --write",
    "content:build:recipes": "node tools/content-build/build-recipes-registry.mjs --write",
    "content:build": "npm run content:build:resources && npm run content:build:processes && npm run content:build:recipes",
    "content:check:resources": "node tools/content-build/build-resources-registry.mjs --check",
    "content:check:processes": "node tools/content-build/build-processes-registry.mjs --check",
    "content:check:recipes": "node tools/content-build/build-recipes-registry.mjs --check",
    "content:check-graph": "node tools/content-build/check-content-graph.mjs",
    "content:check": "npm run content:check:resources && npm run content:check:processes && npm run content:check:recipes && npm run content:check-graph",
    "content:validate": "npm run content:check",
    "quality:check": "npm run architecture:check && npm run filesize:check && npm run content:check",
    "test": "vitest run --exclude scripts/.tmp/**",
    "wiki:sync": "tsc -p scripts/tsconfig.wiki.json && node --experimental-specifier-resolution=node scripts/.tmp/scripts/wiki-sync.js --write",
    "wiki:check": "tsc -p scripts/tsconfig.wiki.json && node --experimental-specifier-resolution=node scripts/.tmp/scripts/wiki-sync.js --check",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@supabase/supabase-js": "^2.97.0",
    "@tanstack/react-virtual": "^3.13.18",
    "babylonjs": "^8.51.2",
    "dexie": "^4.3.0",
    "flexsearch": "^0.8.212",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@mdx-js/rollup": "^3.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
